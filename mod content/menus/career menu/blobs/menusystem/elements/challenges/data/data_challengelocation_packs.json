{
    "Season": "packs",
    "ChallengeData": {
        "$arrayfilter": {
            "source": "$.ProfileData.ChallengeData.Children",
            "addCondition": {
                "$and": [
                    "$eq($.Location.Id,LOCATION_PARENT_ICA_FACILITY)",
                    "$not $eq($.Location.DisplayNameLocKey,$.Name)"
                ]
            }
        }
    }
}
