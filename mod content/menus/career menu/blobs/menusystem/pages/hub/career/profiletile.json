{
    "id": "$.Title",
    "controller": "contextitem",
    "view": "menu3.basic.MenuTileTall",
    "row": -0.75,
    "nrows": 0.5,
    "ncols": 2,
    "data": {
        "title": "$.Title",
        "header": "$loc UI_MENU_PAGE_HUB_PROFILE",
        "icon": "$.Icon",
        "image": "$.image"
    },
    "actions": {
        "$mergeobjects": [
            {
                "$if $not $isnull $.ExtraActions": {
                    "$then": "$expand $.ExtraActions"
                }
            },
            {
                "select": [
                        {
                            "replace-children": {
                    "target": "headline_container",
                    "children": [
                        {
                            "view": "menu3.basic.HeadlineElement",
                            "selectable": false,
                            "pressable": false,
                            "data": {
                                "header": "$loc UI_MENU_PAGE_HUB_PROFILE",
                                "title": "$.Title",
                                "typeicon": "$.Icon"
                            }
                        }
                    ]
                }
                        }
                    ]
                },
                {
                    "deselect": [
                        {
                                                "replace-children": {
                    "target": "headline_container",
                    "children": [
                        {
                            "view": "menu3.basic.HeadlineElement",
                            "selectable": false,
                            "pressable": false
                        }
                    ]
                }
                        }
                    ]
                }
        ]
    }
}
